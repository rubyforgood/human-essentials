<section class="content-header">
	<% content_for :title, "#{current_organization.name} - Inventory - Barcode Items - New" %>
  <h1>
    New Barcode Item
    <small>for <%= current_organization.name %></small>
  </h1>
	<ol class="breadcrumb">
	<li><%= link_to(dashboard_path) do %>
	    <i class="fa fa-dashboard"></i> Home
	  <% end %>
	  </li>
	  <li><%= link_to "Barcode Items", (barcode_items_path) %></li>  
	  <li class="active"> New Barcode</li>
	</ol>
</section>

<!-- Main content -->
<section class="content">

	<!-- Default box -->
	<div class="box">
	  <div class="box-header with-border">
	    <h3 class="box-title">New Barcode</h3>
	  </div>
	  <div class="box-body">
			<%= render partial: "form", object: @barcode_item %>
	  </div>
	</div>
	<!-- /.box -->

</section>
