<section class="content-header">
<% content_for :title, "New - Partners - Agencies - #{current_organization.name}" %>
<h1>
  Partner Info
  <small>for <%= current_organization.name %></small>
</h1>
<ol class="breadcrumb">
<li><%= link_to(dashboard_path) do %>
    <i class="fa fa-dashboard"></i> Home
  <% end %>
  </li>
  <li><%= link_to "Partners", (partners_path) %></li>
  <li class="active"> New Partner</li>
</ol>
</section>

<!-- Main content -->
<section class="content">

<!-- Default box -->
<div class="box">
  <div class="box-header with-border">
    <h3 class="box-title">New Partner Agency</h3>
  </div>
  <div class="box-body">

  <h1 class="medium-offset-3 medium-6"></h1>

  <%= render partial: "form", object: @partner, locals: { submit_btn_options: { text: "Add Partner Agency" } } %>
  </div>
</div>
<!-- /.box -->

</section>
