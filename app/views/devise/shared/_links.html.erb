<%- if controller_name != 'sessions' %>
  <br />
  <%= link_to "Log in", new_session_path(resource_name), class: "btn btn-success" %><br />
<% end -%>
<%- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations' %>
  <br />
  <%= link_to "Forgot your password?", new_password_path(resource_name), class: "btn btn-success" %><br />
<% end -%>

