<section class="content-header">
  <% content_for :title, "Canonical Items - New" %>
  <h1>
    New Canonical Item
  </h1>
  <ol class="breadcrumb">
  <li><%= link_to(dashboard_path) do %>
      <i class="fa fa-dashboard"></i> Home
    <% end %>
    </li>
    <li><%= link_to "All Canonical Items", (admin_canonical_items_path) %></li>
    <li class="active"> New Canonical Item</li>
  </ol>
</section>

<!-- Main content -->
<section class="content">

  <!-- Default box -->
  <div class="box">
    <div class="box-header with-border">
      <h3 class="box-title">New Canonical Item</h3>
    </div>
    <div class="box-body">
      <%= render partial: "form", object: @canonical_item, locals: { submit_text: "Create Canonical Item" } %>
    </div>
  </div>
  <!-- /.box -->

</section>


